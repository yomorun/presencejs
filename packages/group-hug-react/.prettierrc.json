{
    "jsxSingleQuote": true,
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "proseWrap": "never",
    "bracketSpacing": true,
    "quoteProps": "preserve",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "ignore",
    "plugins": ["prettier-plugin-tailwindcss"]
}